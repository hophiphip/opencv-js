{
  "name": "@hophiphip/opencv-js",
  "version": "0.0.3",
  "main": "opencv.js",
  "description": "OpenCV for JavaScript compiled with Emscripten",
  "bundledDependencies": false,
  "scripts": {
    "build": "mkdir -p build && docker build -t emsdk-for-opencv ./docker/",

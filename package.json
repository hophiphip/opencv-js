    "type": "git",
    "url": "https://github.com/hophiphip/opencv-js.git"
  },
  "bugs": {
    "url": "https://github.com/hophiphip/opencv-js/issues"
  },
  "homepage": "https://github.com/hophiphip/opencv-js#readme"
}
